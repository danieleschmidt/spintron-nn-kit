# SpinTron-NN-Kit Environment Configuration

# Development Environment
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO

# Device Simulation Settings
SPICE_SIMULATOR=ngspice
MTJ_MODEL_PATH=./models/stanford_mtj_model.lib
DEFAULT_MTJ_RESISTANCE_HIGH=10000
DEFAULT_MTJ_RESISTANCE_LOW=5000
DEFAULT_SWITCHING_VOLTAGE=0.3

# Hardware Generation
DEFAULT_TARGET_FREQUENCY=50000000
DEFAULT_CORE_VOLTAGE=0.8
DEFAULT_IO_VOLTAGE=1.8
VERILOG_OUTPUT_DIR=./output/verilog

# Training Configuration
PYTORCH_DEVICE=auto
TORCH_HOME=./cache/torch
WANDB_PROJECT=spintron-nn-kit
WANDB_API_KEY=your_wandb_api_key_here

# Simulation and Testing
SIMULATION_TEMP_DIR=./tmp/simulation
TEST_DATA_DIR=./test_data
BENCHMARK_OUTPUT_DIR=./benchmarks/results

# Power Analysis
POWER_ANALYSIS_TOOL=primetime
POWER_LIB_PATH=./libs/power

# Security and API Keys
GITHUB_TOKEN=your_github_token_here
DOCKER_REGISTRY_URL=your_registry_url_here
DOCKER_REGISTRY_USERNAME=your_username_here
DOCKER_REGISTRY_PASSWORD=your_password_here

# Database (if using for metrics tracking)
DATABASE_URL=sqlite:///./spintron_metrics.db

# External Tool Integration
CADENCE_LICENSE_FILE=your_cadence_license_path
SYNOPSYS_LICENSE_FILE=your_synopsys_license_path
XILINX_LICENSE_FILE=your_xilinx_license_path

# Performance Monitoring
ENABLE_PERFORMANCE_MONITORING=true
TELEMETRY_ENDPOINT=https://your-telemetry-endpoint.com